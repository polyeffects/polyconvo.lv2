@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix pset: <http://lv2plug.in/ns/ext/presets#> .
@prefix zcpset: <http://polyeffects.com/lv2/@LV2NAME@/pset#> .

<http://polyeffects.com/lv2/@LV2NAME@#Mono>
	a lv2:Plugin;
	lv2:binary <@LV2NAME@@LIB_EXT@>;
	rdfs:seeAlso <@LV2NAME@.ttl>.

<http://polyeffects.com/lv2/@LV2NAME@#Stereo>
	a lv2:Plugin;
	lv2:binary <@LV2NAME@@LIB_EXT@>;
	rdfs:seeAlso <@LV2NAME@.ttl>.

<http://polyeffects.com/lv2/@LV2NAME@#MonoToStereo>
	a lv2:Plugin;
	lv2:binary <@LV2NAME@@LIB_EXT@>;
	rdfs:seeAlso <@LV2NAME@.ttl>.

<http://polyeffects.com/lv2/@LV2NAME@#ir>
	a lv2:Parameter;
	rdfs:label "Impulse Response";
	rdfs:range atom:Path.

<http://polyeffects.com/lv2/@LV2NAME@#gain>
	a lv2:Parameter;
	rdfs:label "Signal Level Gain";
	rdfs:range atom:Float.

<http://polyeffects.com/lv2/@LV2NAME@#predelay>
	a lv2:Parameter;
	rdfs:label "Pre-delay";
	rdfs:range atom:Int.

<http://polyeffects.com/lv2/@LV2NAME@#channel_gain>
	a lv2:Parameter;
	rdfs:label "Gain per channel";
	rdfs:range atom:Vector.

<http://polyeffects.com/lv2/@LV2NAME@#channel_predelay>
	a lv2:Parameter;
	rdfs:label "Pre-delay per channel";
	rdfs:range atom:Vector.
